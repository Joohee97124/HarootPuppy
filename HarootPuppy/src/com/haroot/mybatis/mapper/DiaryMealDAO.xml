<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.haroot.mybatis.IDiaryMealDAO"> <!-- 부모 노릇을 할 인터페이스 -->
	
	<!-- 다이어리 식사 (meal) 등록 프로시저 호출 -->
	<insert id="addMeal" statementType="CALLABLE">
		{
       	 	call PRC_DIARY_MEAL_INSERT(#{favorite_name,jdbcType=VARCHAR},#{common_start},#{common_end},#{meal_name},#{meal_amount},#{meal_type},#{common_memo,jdbcType=VARCHAR},#{common_photo,jdbcType=VARCHAR},#{sid_code},#{pet_code})
       	}
	</insert>
	
